{
  "manifest_version": 3,
  "name": "My Horoscope Extension",
  "version": "1.0",
  "description": "Get your horoscope based on your zodiac sign.",
  "action": {
    "default_popup": "index.html"
  },
  "permissions": ["https://horoscope-astrology.p.rapidapi.com/"],

  "web_accessible_resources": [
    {
      "resources": ["style.css"],
      "matches": ["<all_urls>"]
    },
    {
      "resources": ["main.js"],
      "matches": ["<all_urls>"]
    }
  ]
}
