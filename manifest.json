{
  "manifest_version": 3,
  "name": "My Horoscope Extension",
  "version": "1.0",
  "description": "Get your horoscope based on your zodiac sign.",
  "action": {
    "default_popup": "popup.html"
  },
  "permissions": ["https://horoscope-astrology.p.rapidapi.com/*"],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "web_accessible_resources": [
    {
      "resources": ["style.css"],
      "matches": ["<all_urls>"]
    },
    {
      "resources": ["main.js"],
      "matches": ["<all_urls>"]
    }
  ]
}